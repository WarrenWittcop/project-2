<header>
    <div class="container">
      <h1>Welcome to Might and Magic 7 character creator!</h1>
      <a class="nav-link" href="/">Home</a> <br></br>
      <% if (currentUser) { %>
        <a class="nav-link" href="/MM7/new">Create a Character</a>
        <li class="nav-link">Welcome <%= currentUser.username %>!</li>
        <li class="nav-link">
          <form action="/sessions?_method=DELETE" method="POST">
            <input type="submit" value="Log Out" />
          </form>
        </li>
        <% } else { %>
        <li><a class="nav-link" href="/users/new">Sign Up</a></li>
        <li><a class="nav-link" href="/sessions/new">Log In</a></li>
        <% } %>
    </div>
  </header>
